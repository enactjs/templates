{
  "name": "my-app",
  "version": "1.0.0",
  "description": "A general template for an Enact Cordova application.",
  "author": "",
  "main": "src/index.js",
  "scripts": {
    "serve": "enact serve",
    "pack": "enact pack -o www",
    "pack-p": "enact pack -p -o www",
    "watch": "enact pack --watch -o www",
    "clean": "enact clean www",
    "lint": "enact lint  -- --ignore-pattern \"www/*\" .",
    "license": "enact license",
    "test": "enact test",
    "test-watch": "enact test --watch"
  },
  "license": "UNLICENSED",
  "private": true,
  "repository": "",
  "enact": {
    "theme": "sandstone",
    "template": "html-template.ejs"
  },
  "cordova": {},
  "eslintConfig": {
    "extends": "enact-proxy"
  },
  "eslintIgnore": [
    "node_modules/*",
    "build/*",
    "www/*"
  ],
  "dependencies": {
    "@enact/core": "^5.0.0-alpha.1",
    "@enact/i18n": "^5.0.0-alpha.1",
    "@enact/sandstone": "^3.0.0-alpha.1",
    "@enact/spotlight": "^5.0.0-alpha.1",
    "@enact/ui": "^5.0.0-alpha.1",
    "@enact/webos": "^5.0.0-alpha.1",
    "ilib": "^14.19.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "eslint-config-enact-proxy": "^1.0.7"
  }
}
