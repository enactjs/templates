{
  "name": "my-app",
  "version": "1.0.0",
  "description": "A general template for an Enact Cordova application.",
  "author": "",
  "main": "src/index.js",
  "scripts": {
    "serve": "enact serve",
    "pack": "enact pack -o www",
    "pack-p": "enact pack -p -o www",
    "watch": "enact pack --watch -o www",
    "clean": "enact clean www",
    "lint": "enact lint  -- --ignore-pattern \"www/*\" .",
    "license": "enact license",
    "test": "enact test start --single-run",
    "test-watch": "enact test start"
  },
  "license": "UNLICENSED",
  "private": true,
  "repository": "",
  "enact": {
    "theme": "moonstone",
    "template": "html-template.ejs"
  },
  "cordova": {},
  "eslintConfig": {
    "extends": "enact"
  },
  "eslintIgnore": [
    "node_modules/*",
    "build/*",
    "www/*"
  ],
  "dependencies": {
    "@enact/core": "^2.0.0",
    "@enact/i18n": "^2.0.0",
    "@enact/moonstone": "^2.0.0",
    "@enact/spotlight": "^2.0.0",
    "@enact/ui": "^2.0.0",
    "@enact/webos": "^2.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
